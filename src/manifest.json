{
  "name": "Delete chat on fite.tv",
  "description": "Deletes the chat block on fite.tv",
  "version": "1.0",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "deletefite@example.com"
    }
  },
  "content_scripts": [
    {
        "matches": ["https://www.fite.tv/watch/*"],
        "js": ["/content_script.js"]
    }
  ]
}